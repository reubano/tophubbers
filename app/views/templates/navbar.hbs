<div class="container-fluid">
	<a class="btn btn-navbar btn-padleft" data-toggle="collapse" data-target=".nav-collapse">
		{{#each items}}<span class="icon-bar"></span>{{/each}}
	</a>
	<a class="brand" href="{{main.href}}">{{main.title}}</a>

	<div class="nav-collapse collapse">
		<ul class="nav">
			{{#each items}}
			<li><a href="{{href}}">{{title}}</a></li>
			{{/each}}
		</ul>
	</div><!--/.nav-collapse -->
</div>
